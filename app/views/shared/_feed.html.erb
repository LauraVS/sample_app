<% if @feed_items.any? %>

	<ol class="microposts">
		<%= render partial: 'shared/feed_item', collection: @feed_items %>
		<%# Rails knows that @feed_items is a list of feeds.
		It iterates through them and renders each of them using _feed_item.html.erb%>        
	</ol>

	<%= will_paginate @feed_items %>
<% end %>